REM @italian
REM MANIPOLAZIONE DI STRIGHE CON SPAZIATURA (1)
REM
REM Questo esempio mostra come aggiungere uno specifico numero di spazi all'interno
REM di una stringa.
REM
REM @include atari,atarixl,c128,c64,coco,coco3,d32,d64,coleco,sg1000,sc3000,pc128op,to8,cpc

    CLS
    
    PRINT "A";SPACE(15);"Z"
    PRINT "|-- 15 spaces --|"
 